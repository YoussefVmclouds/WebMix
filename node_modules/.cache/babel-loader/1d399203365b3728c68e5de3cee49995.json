div id=\"prvArea\">\n\t<div class=\"prvFeedDragDiv\" v-for=\"feed in liveFeeds\" :key=\"feed.id\">\n      \t<video  \n\t\tv-bind:id=\"feed.name\"\n\t\tautoplay=\"true\"\n\t\tmuted=\"muted\"\n\t\theight=\"115vh\"\n\t\twidth=\"200vw\"\n\t\tclass=\"prvFeedDrag\"\n\t\tdraggable=\"true\"\n\t></video>\n\t{{feed.name}} \t\n    </div>\n</div>\n\n</v-card-title>\n</v-card>\n</v-col>\n\n<!-- CG live keys -->\n<v-col\ncols='1'\n>\n<v-card \ncolor=\"secondary\"\nheight=\"95vh\"\n>\n<v-card-title class=\"justify-center\">\n</v-card-title>\n<v-card-subtitle>Status</v-card-subtitle>\n<v-col\ncols=\"12\">\n<h4 class=\"ml-3\">Live \n    <v-icon\n    class=\"\"\n    normal\n    color=\"#00FF00\"\n    v-if=\"this.liveIcon\">\n      mdi-moon-full\n    </v-icon>\n    <v-icon\n    class=\"\"\n    normal\n    v-else>\n      mdi-moon-new\n    </v-icon>\n    </h4>\n  <h4 class=\"ml-3 mb-3\">Rec.   \n    <v-icon\n    class=\"\"\n    normal\n    color=\"#00FF00\"\n    v-if=\"this.recordIcon\">\n      mdi-moon-full\n    </v-icon>\n    <v-icon\n    class=\"\"\n    normal\n    v-else>\n      mdi-moon-new\n    </v-icon>\n    </h4>\n</v-col>\n    <v-divider></v-divider>\n    <v-card-subtitle>CG Keys</v-card-subtitle>\n <v-switch\n    v-model=\"bgKey\"\n    :disabled=\"bgUploadIcon == 0\"\n    class=\"pl-2\"\n    label=\"BG\"\n    color=\"#FF0000\"\n    hide-details\n  ></v-switch>\n<v-switch\n    v-model=\"logoKey\"\n    :disabled=\"logoUploadIcon == 0\"\n    class=\"pl-2\"\n    label=\"Logo\"\n    color=\"#FF0000\"\n    hide-details\n  ></v-switch>\n <v-switch\n    v-model=\"bugKey\"\n    :disabled=\"bugUploadIcon == 0\"\n    class=\"pl-2 mb-5\"\n    label=\"Bug\"\n    color=\"#FF0000\"\n    hide-details\n  ></v-switch>\n<v-divider></v-divider>\n <v-switch\n    v-model=\"s01Key\"\n    :disabled=\"straps[0].liveReady==0\"\n    class=\"pl-2\"\n    label=\"S-01\"\n    color=\"#FF0000\"\n    hide-details\n  ></v-switch>\n <v-switch\n    v-model=\"s02Key\"\n    :disabled=\"straps[1].liveReady==0\"\n  