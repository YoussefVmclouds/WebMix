      </v-list-item-subtitle>\n          </v-list-item-content>\n        </v-list-item>\n      </v-list-group>\n      <!-- End BG/logo/bug -->\n      <!-- Straps -->\n\t<v-list-group\n\t\tclass=\"rightMenu\"\n        active-class=\"rightMenu-active\"\n        color= \"white\"\n        :value=\"false\"\n        prepend-icon=\"mdi-text-box-multiple\"\n        no-action\n        sub-group\n      >\n        <template v-slot:activator>\n          <v-list-item-content>\n            <v-list-item-title>\n              <p class=\"font-weight-bold\">\n                Straps\n              </p>\n            </v-list-item-title>\n          </v-list-item-content>\n        </template>\n\n        <v-list-item>\n          <v-list-item-content>\n            <v-list-item-title>\n                  <v-tabs\n                  v-model=\"tab\"\n                  background-color=\"transparent\"\n                  color=\"#000\"\n                  grow\n                >\n                  <v-tab\n                    v-for=\"strap in straps\"\n                    :key=\"strap.id\"\n                  >\n                    {{ strap.title }}\n                  </v-tab>\n                </v-tabs>\n\n                <v-tabs-items v-model=\"tab\">\n                  <v-tab-item\n                    v-for=\"strap in straps\"\n                    :key=\"strap.id\"\n                  >\n                    <v-card\n                      flat\n                    >\n                    <v-textarea\n                      dark\n                      class=\"strapTextArea mt-5 text--black mx-1\"\n                      color=\"#000\"\n                      v-model=\"strap.textArea\"\n                      outlined\n                      shaped\n                      filled\n                      rows=\"1\"\n                      row-height=\"15\" \n                      label=\"Strap Text\"\n                    ></v-textarea>\n                    <v-row justify=\"space-around\"><h3 style=\"color:black 